{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["react-app", "react-app/jest", "eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  }
}
